{
  "name": "phptek/silverstripe-exodus",
  "description": "An ETL provider for migrating content from any website into a Silverstripe based project.",
  "license": "BSD-3-Clause",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "migrate",
    "migration",
    "migrator",
    "content",
    "content migration",
    "staticsiteconnector",
    "scraper",
    "scrape"
  ],
  "authors": [
    {
      "name": "Russell Michell",
      "email": "russ@theruss.com"
    }
  ],
  "replace": {
    "phptek/staticsiteconnector": "*"
  },
  "autoload": {
    "psr-4": {
      "": "external-content/"
    }
  },
  "require": {
    "silverstripe/cms": "^4",
    "brittainmedia/phpcrawl": "^0.9",
    "electrolinux/phpquery": "dev-master",
    "silverstripe/reports": "^4",
    "symbiote/silverstripe-queuedjobs": "^4"
  },
  "extra": {
    "installer-name": "exodus"
  }
}
