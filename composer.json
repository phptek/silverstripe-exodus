{
  "name": "phptek/silverstripe-exodus",
  "description": "Full automated content migration from any site into Silverstripe.",
  "license": "BSD-3-Clause",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "migrate",
    "migration",
    "migrator",
    "content",
    "staticsiteconnector",
    "connect",
    "exodus",
    "scraper",
    "scrape",
    "ETL",
    "extract",
    "extractor",
    "transform",
    "transformer",
    "load",
    "import",
    "importer"
  ],
  "authors": [
    {
      "name": "Russell Michell",
      "email": "russ@theruss.com"
    },
    {
      "name": "Sam Minnee",
      "email": "sam@silverstripe.com"
    }
  ],
  "replace": {
    "phptek/staticsiteconnector": "*",
    "silverstripe/staticsiteconnector": "*"
  },
  "autoload": {
    "psr-4": {
      "": "external-content/"
    }
  },
  "require": {
    "silverstripe/cms": "^4",
    "silverstripe/reports": "^4",
    "brittainmedia/phpcrawl": "^0.9",
    "phptek/phpquery": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "extra": {
    "installer-name": "exodus",
    "expose": [
      "external-content/javascript",
      "external-content/css",
      "js"
    ]
  }
}
